<%= render :partial => 'common/breadcrumbs', :object => [['Administer Site', admin_path], 'All Order Lists'] %>

<p><%= link_to 'Create a new order list', new_order_list_path %></p>

<h3>Active Order Lists</h3>
<ul class="order_lists">
  <% @active_order_lists.each do |order_list| %>
    <%= render order_list %> 
  <% end %>
</ul>

<h3>Inactive Order Lists</h3>
<%= will_paginate @inactive_order_lists %>

<ul class="order_lists">
  <% @inactive_order_lists.each do |order_list| %>
    <%= render order_list %> 
  <% end %>
</ul>

<%= will_paginate @inactive_order_lists %>